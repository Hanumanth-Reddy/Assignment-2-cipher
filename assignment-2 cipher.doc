Question 3:

var y="karachi";
function replace(str){
for(var i=0;i<str.length;i++){

 if(str[i]==="a"){str=str.replace("a","1");}  
else if(str[i]==="e"){str=str.replace("e","2");} 
else if(str[i]==="i"){str=str.replace("i","3");} 
else if(str[i]==="o"){str=str.replace("o","4");} 
else if(str[i]==="u"){str=str.replace("u","5");}}
return str;
}
replace(y);









